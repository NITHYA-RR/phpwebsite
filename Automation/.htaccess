<IfModule mod_rewrite.c>
    RewriteEngine On

    # Rewrite rule to remove .php extension
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ $1.php [L]

    # Rewrite rule for clean URLs (e.g., /page/123)
    RewriteRule ^page/([0-9]+)$ page.php?id=$1 [L,QSA]

    # Add more rules as needed
</IfModule>
